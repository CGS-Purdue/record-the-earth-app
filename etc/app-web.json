{
  "expo": {
    "name": "RecordTheEarthExpoDevelopment",
    "description": "Updating RtE react-native app to be fully managed with expo.",
    "entryPoint": "init-web.js",
    "icon": "./icon.png",
    "icon": "./Assets/icon.png",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "primaryColor": "#cccccc",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "slug": "record-the-earth-expo",
    "androidShowExponentNotificationInShellApp": true,
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "notification": {
      "icon": "./Assets/icon.png",
      "color": "#000000",
      "androidMode": "default"
    },
    "splash": {
      "image": "./Assets/splash.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "version": "1.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "./Assets/btn/*.png",
      "./Assets/fonts/*.ttf",
      "./ssets/icons/*.png",
      "./Assets/img/*.png",
      "./Assets/img/*.jpg",
      "./Assets/logo/*.png",
      "./Assets/logo/*.svg"
    ],
    "ios": {
      "supportsTablet": false,
      "splash": {
        "backgroundColor": "#000000",
        "resizeMode": "contain"
      }
    },
    "android": {
      "package": "org.recordtheearth.expo",
      "permissions": [
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "githubUrl": "https://github.com/charrismatic/record-the-earth-expo-react-native",
    "extra": {
      "debug_flags": {
        "DEBUG_OUTLINE_STYLES": true
      }
    },
    "hooks": {},
    "rnCliPath": "",
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "jpg",
        "svg",
        "png"
      ]
    }
  }
}
