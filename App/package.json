{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": ". ./.npmrc && REACT_DEBUGGER=\"${REACT_DEBUGGER}\" EXPO_DEBUG=true expo start --localhost --android --dev --minify --max-workers 8 --config ./app.json",
    "build:dev": "expo build:android --config app.json --type apk --release-channel development",
    "build:web": "expo build:web --config ./init-web.js",
    "recon": ".run/adb-devices && .run/adb-reconnect && .run/adb-reverse",
    "rn:debugger": ".run/react-debugger",
    "bundle" : "metro build --verbose --project--roots $PWD --dev --platform android --out ./bundle.js",
    "start": "expo start",
    "android": "expo start --android",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "testRegex": "/__tests__/.*\\.(test|spec)\\.(js|tsx?)$"
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^10.0.3",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "expo": "^35.0.0",
    "expo-application": "^1.0.0",
    "expo-asset": "^7.0.0",
    "expo-av": "^7.0.1",
    "expo-constants": "^7.0.0",
    "expo-file-system": "^7.0.0",
    "expo-font": "^7.0.0",
    "expo-linear-gradient": "^7.0.0",
    "expo-web-browser": "^7.0.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-svg": "^9.9.9",
    "react-native-web": "^0.11.7",
    "react-navigation": "^3.13.0",
    "react-navigation-stack": "^1.9.3",
    "react-navigation-tabs": "^2.5.5"
  },
  "devDependencies": {
    "babel-preset-expo": "^7.0.0",
    "jest-expo": "^35.0.0"
  },
  "private": true
}
