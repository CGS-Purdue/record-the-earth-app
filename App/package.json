{
  "name": "record-the-earth-react-native",
  "version": "0.9.1",
  "description": "record the earth react native project files for expo and snack",
  "author": "Matt Harris <charrismatic@protonmail.com> (https://charrismatic.github.io/)",
  "main": "./index.js",
  "keywords": [
    "react-native",
    "sound-recording",
    "expo",
    "snack"
  ],
  "scripts": {
    "adb": ".bin/_adb",
    "adb.devices": ".run/adb-devices",
    "adb.get-serial": ".run/adb-get-serial && npm run adb.devices",
    "adb.log": ".run/adb-log",
    "adb.ls": ".bin/_adb devices",
    "adb.mirror": ".run/adb-mirror",
    "adb.recon": ".run/adb-devices && .run/adb-reconnect && .run/adb-reverse",
    "adb.reconnect": ".run/adb-reconnect && npm run adb.devices",
    "adb.reverse": ".run/adb-reverse",
    "adb.shake": ".run/adb-shake",
    "bump": "npm version patch",
    "bundle": "metro build --verbose --project--roots $PWD --dev --platform android --out ./bundle.js",
    "config:linters": ".run/config-linters",
    "config:compgen": ". .run/config-compgen",
    "dev:debug": ". ./.npmrc && EXPO_DEBUGGER=\"${EXPO_DEBUGGER}\" EXPO_DEBUG=true expo start --localhost --android --max-workers 8 --config ./app.json",
    "dev:default": ". ./.npmrc && expo start --localhost --android --dev --no-minify --clear --max-workers 8 --config ./app.json",
    "dev:hot": ". ./.npmrc && expo start --localhost --android --dev --max-workers 8 --config ./app.json",
    "dev:restart": ".run/dev-restart",
    "dev:web": ".run/dev-web",
    "dev": ".run/dev",
    "expo:start": ".run/_expo",
    "expo:debug": "./expo.js",
    "expo:web": "expo start --web",
    "expo:android": "expo start --android",
    "expo:builddev": "expo build:android --config app.json --type apk --release-channel development",
    "expo:build:iosdev": ".run/expo-build-ios-dev",
    "expo:build:web": "expo build:web --config ./init-web.js",
    "env:reset": ".run/env-reset",
    "env:user": ".run/env-print",
    "eslint-check": "./node_modules/.bin/eslint --print-config ./App.js |  ./node_modules/.bin/eslint-config-prettier-check",
    "format": "prettier-eslint \"src/**/*.js\"",
    "info": ".bin/_info",
    "jest": "node_modules/.bin/jest",
    "jsonlint": "jsonlint -s -p",
    "lint.proj": ".run/lint-project",
    "lint.watch": ".run/lint-watch",
    "lint": ".bin/_lint",
    "metro:bundle": ".run/metro-bundle",
    "metro:depends": ".run/metro-depends",
    "metro:export": "node_modules/.bin/metro build app.json --out .build/bundle.js",
    "metro": "node_modules/.bin/metro",
    "pretty:check": ".run/pretty-check",
    "pretty:fix": ".run/pretty-fix",
    "pretty:list": ".run/pretty-list",
    "pretty": ".bin/_pretty",
    "rn:bundle": ".run/rn-bundle",
    "rn:debugger": ".run/react-debugger",
    "test": "jest"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/async-storage": "^1.6.2",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@unimodules/core": "~4.0.0",
    "expo": "^35.0.0",
    "expo-asset": "~7.0.0",
    "expo-av": "~7.0.0",
    "expo-blur": "~7.0.0",
    "expo-constants": "~7.0.0",
    "expo-file-system": "~7.0.0",
    "expo-font": "~7.0.0",
    "expo-intent-launcher": "~7.0.0",
    "expo-linear-gradient": "~7.0.0",
    "expo-web-browser": "~7.0.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "~2.10.0",
    "react-native-svg": "~9.9.2",
    "react-native-web": "^0.11.7",
    "react-native-webview": "7.0.5",
    "react-navigation": "^3.13.0",
    "react-navigation-stack": "^1.5.3",
    "react-navigation-tabs": "^1.2.0",
    "rollup": "^1.23.1",
    "expo-application": "~1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@expo/webpack-config": "^0.7.12",
    "@react-native-community/eslint-config": "0.0.5",
    "@typescript-eslint/parser": "^1.13.0",
    "acorn": "^6.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^7.0.0",
    "eslint": "5.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.4.1",
    "jest-expo": "^35.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "typescript": "^3.4.5"
  },
  "bugs": {
    "url": "https://github.com/charrismatic/record-the-earth-react-native-snack/issues"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "private": true
}
