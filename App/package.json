{
  "name": "record-the-earth-react-native",
  "version": "0.0.2",
  "description": "record the earth react native project files for expo and snack",
  "author": "Matt Harris <charrismatic@protonmail.com> (https://charrismatic.github.io/)",
  "scripts": {
    "adb": ".bin/_adb",
    "start": ".run/_expo",
    "watch": "./.run/watch",
    "lint": ".bin/eslint",
    "expo": ".bin/_expo",
    "expo:start": ".run/expo-start",
    "adb:log": ".run/adb-log",
    "adb:devices": ".run/adb-devices",
    "adb:get-serial": ".run/adb-get-serial && npm run adb:devices",
    "adb:reconnect": ".run/adb-reconnect && npm run adb:devices",
    "adb:reverse": ".run/adb-reverse",
    "adb:ls": ".bin/_adb devices",
    "bump": "npm version patch",
    "dev": ".run/dev",
    "env:reset": ".run/env-reset",
    "env:user": ".run/env-print",
    "env:npm": ".run/env-print-npm",
    "info": "./.run/info.js",
    "info:env": "./.run/info.js -- env",
    "info:scripts": "./.run/info.js -- scripts",
    "lint:dir": ".run/eslint ./",
    "lint:project": ".run/eslint-project",
    "lint:watch": ".run/eslint-watch",
    "pretty:check": ".run/prettier --check",
    "pretty:fix": ".run/prettier --write",
    "pretty:list": ".run/prettier --list",
    "util:reactotron": ".run/react-reactotron",
    "util:devtools": ".run/react-devtools",
    "util:debugger": ".run/react-debugger"
  },
  "react-devtool": "react-devtools",
  "keywords": [
    "react-native",
    "sound-recording",
    "expo",
    "snack"
  ],
  "eslintConfig": {
    "extends": "@react-native-community/eslint-config"
  },
  "watch": {
    "lint": {
      "patterns": [
        "*.js",
        "Components",
        "Lib",
        "LifeCycle",
        "Theme",
        "Utilities",
        "Views",
        "Screens"
      ],
      "recursive": true,
      "ignore": "node_modules",
      "quiet": false,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": false
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.3",
    "@types/react-native": "^0.60.9",
    "expo": "^34.0.0",
    "expo-asset": "^6.0.0",
    "expo-av": "^6.0.0",
    "expo-constants": "6.0.0",
    "expo-file-system": "^6.0.2",
    "expo-font": "~6.0.0",
    "expo-web-browser": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-modal": "^11.3.1",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "^1.1.0",
    "react-native-swipe-cards": "^0.1.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0",
    "typescript": "^3.6.2"
  },
  "bugs": {
    "url": "https://github.com/charrismatic/record-the-earth-react-native-snack/issues"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "0.0.5",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "node-watch": "^0.6.3",
    "react-devtools": "^3.6.3",
    "reactotron-core-client": "^2.8.3",
    "reactotron-react-native": "^3.6.4",
    "reactotron-redux": "^3.1.1",
    "redux": "^4.0.4",
    "remote-redux-devtools": "^0.5.16"
  },
  "private": true
}
