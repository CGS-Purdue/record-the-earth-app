{
  "name": "record-the-earth-react-native",
  "version": "0.0.5",
  "description": "record the earth react native project files for expo and snack",
  "author": "Matt Harris <charrismatic@protonmail.com> (https://charrismatic.github.io/)",
  "scripts": {
    "adb": ".bin/_adb",
    "beautify": ".bin/_beautify",
    "rollup": ".bin/_rollup",
    "expo": ".bin/_expo",
    "expo.auth": ".run/expo-auth",
    "expo.start": ".run/expo-start",
    "expo.web": "expo start --web-only --dev --localhost",
    "lint": ".bin/_lint",
    "info": ".bin/info.js",
    "pretty": ".bin/_pretty",
    "run:start": ".run/_expo",
    "adb.devices": ".run/adb-devices",
    "adb.get-serial": ".run/adb-get-serial && npm run adb.devices",
    "adb.log": ".run/adb-log",
    "adb.ls": ".bin/_adb devices",
    "adb.mirror": ".run/adb-mirror",
    "adb.reconnect": ".run/adb-reconnect && npm run adb.devices",
    "adb.reverse": ".run/adb-reverse",
    "adb.shake": ".run/adb-shake",
    "connect": ".bin/_adb connect",
    "bump": "npm version patch",
    "dev": ".run/dev",
    "dev:web": ".run/dev-web",
    "dev:restart": ".run/dev-restart",
    "env:reset": ".run/env-reset",
    "env:user": ".run/env-print",
    "env:npm": ".run/env-print-npm",
    "info:scripts": ".run/info.js -- scripts",
    "jstyles:extract": ".run/jstyles-extract-stylsheet",
    "watch": ".run/watch",
    "lint.watch": ".run/lint-watch",
    "lint.proj": ".run/lint-project",
    "pretty:check": ".run/pretty-check",
    "pretty:list": ".run/pretty-list",
    "pretty:fix": ".run/pretty-fix",
    "react:reactotron": ".run/react-reactotron",
    "react:debugger": ".run/react-debugger",
    "format": "prettier-eslint \"src/**/*.js\"",
    "metro": "node_modules/.bin/metro",
    "metro:depends": ".run/metro-depends",
    "metro:bundle": ".run/metro-bundle",
    "rn:bundle": ".run/rn-bundle",
    "jest": "node_modules/.bin/jest",
    "eslint-check": "./node_modules/.bin/eslint --print-config ./App.js |  ./node_modules/.bin/eslint-config-prettier-check"
  },
  "react-devtool": "react-devtools",
  "keywords": [
    "react-native",
    "sound-recording",
    "expo",
    "snack"
  ],
  "eslintConfig": {
    "extends": "@react-native-community/eslint-config"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.3",
    "@types/react-native": "^0.60.15",
    "expo": "^35.0.0",
    "expo-application": "^1.0.0",
    "expo-asset": "^7.0.0",
    "expo-av": "^7.0.1",
    "expo-blur": "^7.0.0",
    "expo-constants": "^7.0.0",
    "expo-file-system": "^7.0.0",
    "expo-font": "^7.0.0",
    "expo-intent-launcher": "^7.0.0",
    "expo-linear-gradient": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-svg": "^9.9.5",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.9.0",
    "react-navigation-tabs": "^2.5.5",
    "react-redux": "^7.1.1"
  },
  "bugs": {
    "url": "https://github.com/charrismatic/record-the-earth-react-native-snack/issues"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.6.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "5.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.1.2",
    "jest-expo": "^35.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "typescript": "^3.6.3"
  },
  "private": true,
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
