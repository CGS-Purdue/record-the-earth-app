{
  "name": "record-the-earth-react-native",
  "version": "0.9.1",
  "description": "record the earth react native project files for expo and snack",
  "author": "Matt Harris <charrismatic@protonmail.com> (https://charrismatic.github.io/)",
  "main": "node_modules/expo/AppEntry.js",
  "keywords": [
    "react-native",
    "sound-recording",
    "expo",
    "snack"
  ],
  "scripts": {
    "adb": ".bin/_adb",
    "dev": ". ./.npmrc && expo start --localhost --android --dev --minify --max-workers 8 --config ./app.json",
    "dev:hot": ". ./.npmrc && expo start --localhost --android --dev --max-workers 8 --config ./app.json",
    "dev:debug": ". ./.npmrc && EXPO_DEBUGGER=\"${EXPO_DEBUGGER}\" EXPO_DEBUG=true expo start --localhost --android --dev --minify --max-workers 8 --config ./app.json",
    "build:dev": "expo build:android --config app.json --type apk --release-channel development",
    "build:web": "expo build:web --config ./init-web.js",
    "config:linters": ".run/config-linters",
    "recon": ".run/adb-devices && .run/adb-reconnect && .run/adb-reverse",
    "bundle": "metro build --verbose --project--roots $PWD --dev --platform android --out ./bundle.js",
    "lint": ".bin/_lint",
    "pretty": ".bin/_pretty",
    "bump": "npm version patch",
    "lint:project": ".run/lint-project",
    "lint:watch": ".run/lint-watch",
    "metro:export": "node_modules/.bin/metro build app.json --out .build/bundle.js",
    "metro": "node_modules/.bin/metro",
    "rn:debugger": ".run/react-debugger",
    "start": "expo start",
    "android": "expo start --android",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "testRegex": "/__tests__/.*\\.(test|spec)\\.(js|tsx?)$"
  },
  "dependencies": {
    "@eva-design/eva": "^1.2.0",
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^10.0.3",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "expo": "^35.0.0",
    "expo-application": "^1.0.0",
    "expo-asset": "^7.0.0",
    "expo-av": "^7.0.1",
    "expo-blur": "^7.0.0",
    "expo-constants": "^7.0.0",
    "expo-file-system": "^7.0.0",
    "expo-font": "^7.0.0",
    "expo-intent-launcher": "^7.0.0",
    "expo-linear-gradient": "^7.0.0",
    "expo-web-browser": "^7.0.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-svg": "^9.9.9",
    "react-native-ui-kitten": "^4.2.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "^3.13.0",
    "react-navigation-stack": "^1.9.3",
    "react-navigation-tabs": "^2.5.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.6.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^7.0.0",
    "eslint": "5.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.1.2",
    "jest-expo": "^35.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "typescript": "^3.6.3"
  },
  "bugs": {
    "url": "https://github.com/charrismatic/record-the-earth-react-native-snack/issues"
  },
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "private": true
}
