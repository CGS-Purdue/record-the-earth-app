{
  "name": "record-the-earth-react-native",
  "version": "0.0.2",
  "description": "record the earth react native project files for expo and snack",
  "author": "Matt Harris <charrismatic@protonmail.com> (https://charrismatic.github.io/)",
  "scripts": {
    "adb": ".bin/_adb",
    "beautify": ".bin/_beautify",
    "expo": ".bin/_expo",
    "lint": ".bin/_lint",
    "info": ".bin/info.js",
    "pretty": ".bin/_pretty",
    "run:start": ".run/_expo",
    "expo:start": ".run/expo-start",
    "adb:log": ".run/adb-log",
    "adb:devices": ".run/adb-devices",
    "adb:shake": ".run/adb-shake",
    "adb:get-serial": ".run/adb-get-serial && npm run adb:devices",
    "adb:reconnect": ".run/adb-reconnect && npm run adb:devices",
    "adb:reverse": ".run/adb-reverse",
    "adb:ls": ".bin/_adb devices",
    "bump": "npm version patch",
    "dev": ".run/dev",
    "dev:web": ".run/dev-web",
    "expo.web": "expo start --web-only --dev --localhost",
    "dev:restart": ".run/dev-restart",
    "env:reset": ".run/env-reset",
    "env:user": ".run/env-print",
    "env:npm": ".run/env-print-npm",
    "info:scripts": "./.run/info.js -- scripts",
    "watch": "./.run/watch",
    "lint.watch": ".run/lint-watch",
    "lint.proj": ".run/lint-project",
    "pretty:check": ".run/pretty-check",
    "pretty:list": ".run/pretty-list",
    "pretty:fix": ".run/pretty-fix",
    "react:reactotron": ".run/react-reactotron",
    "react:devtools": ".run/react-devtools",
    "react:debugger": ".run/react-debugger",
    "format": "prettier-eslint \"src/**/*.js\"",
    "metro:export" : "node_modules/.bin/metro build app.json --out .build/bundle.js",
    "metro" : "node_modules/.bin/metro",
    "eslint-check": "./node_modules/.bin/eslint --print-config ./App.js |  ./node_modules/.bin/eslint-config-prettier-check"
  },
  "react-devtool": "react-devtools",
  "keywords": [
    "react-native",
    "sound-recording",
    "expo",
    "snack"
  ],
  "eslintConfig": {
    "extends": "@react-native-community/eslint-config"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.3",
    "@types/react-native": "^0.60.15",
    "babel-plugin-optional-require": "^0.3.1",
    "expo": "^34.0.0",
    "expo-asset": "^6.0.0",
    "expo-av": "^6.0.0",
    "expo-blur": "^6.0.0",
    "expo-constants": "6.0.0",
    "expo-file-system": "^6.0.2",
    "expo-font": "~6.0.0",
    "expo-linear-gradient": "^6.0.0",
    "expo-web-browser": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-animatable": "^1.3.2",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-modal": "^11.3.1",
    "react-native-paper": "^2.16.0",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "^1.1.0",
    "react-native-swipe-cards": "^0.1.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0",
    "styled-components": "^4.3.2",
    "typescript": "^3.6.3"
  },
  "bugs": {
    "url": "https://github.com/charrismatic/record-the-earth-react-native-snack/issues"
  },
  "devDependencies": {
    "@expo/webpack-config": "^0.7.4",
    "@getify/eslint-plugin-proper-arrows": "^8.0.1",
    "@react-native-community/eslint-config": "0.0.5",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "node-watch": "^0.6.3",
    "prettier-eslint": "^9.0.0",
    "react-devtools": "^3.6.3",
    "react-native-svg": "^9.9.4",
    "reactotron-core-client": "^2.8.4",
    "reactotron-react-native": "^3.6.4",
    "reactotron-redux": "^3.1.1",
    "redux": "^4.0.4",
    "remote-redux-devtools": "^0.5.16"
  },
  "private": true,
  "bit": {
    "env": {},
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
